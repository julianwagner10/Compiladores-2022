ControlComoExpresionBienDef{
i32 id,x,z;
f32 a1;
x =: 1;
a1 =: for (id=:2 ; id<=4 ; +2){
        z=: 2;
        if(x>z)then
            break 2.0;
        end_if;
      }
      else
        46.0;
      ;
if (a1 = 46.0) then
    out('es 46');
else
    if( a1 = 2.0) then
        out('es 2');
    else
        out('es otro');
    end_if;
end_if;
}