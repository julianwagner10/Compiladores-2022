FuncionBienDeclarada{
i32 x;
fun f1(i32 y,i32 v):i32{
    i32 x;
    fun f2(f32 z):i32{
        i32 x;
        if(x<y)then{
            x =:y;
            y=: y + 2;
        }
        else
            y=: x;
        end_if;
        return y;
    }
    x =: f2(2.5);
    fun f3(i32 w,f32 p):i32{
        if(w<y)then{
            y=: y + 2;
            w =:y;
            p=: p - w;
        }
        end_if;
        return w;
    }
    y =: f3(45,3.4);
    return x;
}

x=:f1(21,4);

}
