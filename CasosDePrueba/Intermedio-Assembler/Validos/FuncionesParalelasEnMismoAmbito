Funciones{
i32 y;
f32 k;
y=:10;
fun f2(f32 z):i32{
    i32 x;
    x=:0;
    if(x<y)then{
        out('x es menor a y');
        x =:y;
        y=: y + 2;
    }
    else{
        y=: x;
        out('x no es menor a y');
    }
    end_if;
    return y/x;
}
fun f3(i32 w):f32{
    f32 t;
    if(w<y)then{
        out('w es menor a y');
        y=: y + 2;
        w =:y;
    }
    end_if;
    out('w no es menor a y');
    return t;
}

discard f2(25.5);
k =: f3(24);
}
