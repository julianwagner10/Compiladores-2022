FuncionBienDeclarada{
fun f1(i32 y,i32 v):i32{
    i32 x;
    fun f2(f32 z):i32{
        i32 x;
        if(x<y)then{
            x =:y;
            y=: y + 2;
        }
        else
            y=: x;
        end_if;
        return y/x;
    }
    x =: x+3;
    fun f3(i32 w):f32{
        f32 t;
        if(w<y)then{
            y=: y + 2;
            w =:y;
        }
        end_if;
        return t;
    }
    return x;
}

discard f1(25,45);

}
